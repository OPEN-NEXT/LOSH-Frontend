{
  "name": "losh-frontend",
  "private": true,
  "description": "Library of Open Source Hardware frontend package",
  "version": "0.1.0",
  "author": "Adam Hammad <mail@adamham.dev>",
  "dependencies": {
    "antd": "^4.16.8",
    "axios": "^0.21.1",
    "eslint-loader": "^4.0.2",
    "gatsby": "^3.9.1",
    "gatsby-image": "^3.9.0",
    "gatsby-plugin-antd": "^2.2.0",
    "gatsby-plugin-less": "^5.9.0",
    "gatsby-plugin-manifest": "^3.9.0",
    "gatsby-plugin-offline": "^4.9.0",
    "gatsby-plugin-react-helmet": "^4.9.0",
    "gatsby-plugin-sharp": "^3.9.0",
    "gatsby-plugin-typescript": "^3.9.0",
    "gatsby-plugin-web-font-loader": "^1.0.4",
    "gatsby-source-filesystem": "^3.9.0",
    "gatsby-transformer-remark": "^4.6.0",
    "gatsby-transformer-sharp": "^3.9.0",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "rehype-react": "^6.2.1",
    "wikidata-sdk": "^7.13.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@testing-library/cypress": "^8.0.0",
    "@types/jest": "^26.0.24",
    "@types/react": "^17.0.14",
    "@types/react-dom": "^17.0.9",
    "@types/react-helmet": "^6.1.2",
    "@types/react-test-renderer": "^17.0.1",
    "@typescript-eslint/eslint-plugin": "^4.28.4",
    "@typescript-eslint/parser": "^4.28.4",
    "babel-jest": "^27.0.6",
    "babel-preset-gatsby": "^1.9.0",
    "cross-env": "^7.0.3",
    "cypress": "^8.0.0",
    "eslint": "^7.31.0",
    "eslint-plugin-react": "^7.24.0",
    "gatsby-plugin-eslint": "^3.0.0",
    "gatsby-plugin-ts-checker": "^1.1.0",
    "husky": "^7.0.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.0.6",
    "lint-staged": "^11.0.1",
    "prettier": "^2.3.2",
    "pretty-quick": "^3.1.1",
    "react-test-renderer": "^17.0.2",
    "start-server-and-test": "^1.12.6",
    "typescript": "^4.3.5"
  },
  "keywords": [
    "losh",
    "open source",
    "hardware",
    "gatsby",
    "typescript",
    "eslint",
    "jest",
    "prettier"
  ],
  "license": "GPLv3",
  "scripts": {
    "build": "gatsby build",
    "clean": "gatsby clean",
    "develop": "gatsby develop",
    "precommit": "lint-staged",
    "format": "prettier --check \"**/*.{css,js,jsx,json,md,ts,tsx,yml}\"",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "prettier": "prettier --write \"**/*.{css,js,jsx,json,md,ts,tsx,yml}\"",
    "start": "yarn develop",
    "serve": "gatsby serve",
    "test": "jest",
    "cy:open": "cypress open",
    "husky": "husky install",
    "test:e2e": "start-server-and-test develop http://localhost:8000 cy:open",
    "test:e2e:ci": "start-server-and-test develop http://localhost:8000 cy:run"
  },
  "husky": {
    "hooks": {
      "pre-commit": [
        "lint-staged",
        "commitlint --edit '$1'"
      ]
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "yarn lint:fix",
      "yarn prettier"
    ],
    "*.{scss,css,json}": [
      "yarn prettier"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wmde/LOSH-Frontend"
  }
}
